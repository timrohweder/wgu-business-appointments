<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="700.0" prefWidth="960.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View_Controller.CustomerProfileViewController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Schedule Software">
         <font>
            <Font name="Arial Bold" size="30.0" />
         </font>
      </Label>
      <HBox layoutX="476.0" layoutY="14.0" prefHeight="100.0" prefWidth="470.0" spacing="15.0">
         <children>
            <Button mnemonicParsing="false" onAction="#handleAppointmentsClick" prefHeight="27.0" prefWidth="126.0" text="Appointments" />
            <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#handleCustomersClick" prefHeight="27.0" prefWidth="126.0" text="Customers" />
            <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#handleCalendarClick" prefHeight="27.0" prefWidth="126.0" text="Calendar" />
            <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#handleReportsClick" prefHeight="27.0" prefWidth="126.0" text="Reports" />
         </children>
      </HBox>
      <Button layoutX="870.0" layoutY="659.0" mnemonicParsing="false" onAction="#handleExitClick" prefHeight="27.0" prefWidth="76.0" text="Exit" />
      <Label layoutX="14.0" layoutY="91.0" text="Customer Profile">
         <font>
            <Font name="Arial Bold" size="18.0" />
         </font>
      </Label>
      <VBox layoutX="14.0" layoutY="131.0" prefHeight="542.0" prefWidth="703.0" spacing="10.0">
         <children>
            <Label text="Customer Id">
               <font>
                  <Font name="Arial Bold" size="13.0" />
               </font>
            </Label>
            <Label fx:id="customerIdLabel" text="Label" />
            <Label text="Name">
               <font>
                  <Font name="Arial Bold" size="13.0" />
               </font>
            </Label>
            <Label fx:id="customerNameLabel" text="Label" />
            <Label text="Address">
               <font>
                  <Font name="Arial Bold" size="13.0" />
               </font>
            </Label>
            <Label fx:id="addressLabel" text="Label" />
            <Label text="Address #2">
               <font>
                  <Font name="Arial Bold" size="13.0" />
               </font>
            </Label>
            <Label fx:id="address2Label" text="Label" />
            <Label text="City">
               <font>
                  <Font name="Arial Bold" size="13.0" />
               </font>
            </Label>
            <Label fx:id="cityLabel" text="Label" />
            <Label text="Postal Code">
               <font>
                  <Font name="Arial Bold" size="13.0" />
               </font>
            </Label>
            <Label fx:id="postalCodeLabel" text="Label" />
            <Label text="Phone">
               <font>
                  <Font name="Arial Bold" size="13.0" />
               </font>
            </Label>
            <Label fx:id="phoneLabel" text="Label" />
         </children>
      </VBox>
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</AnchorPane>
